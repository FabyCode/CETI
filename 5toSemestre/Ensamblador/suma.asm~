	MOV	P1, #0001
	LCALL	RET
	MOV	R3, P3
	MOV	P1, #0002
	LCALL	RET
	MOV	R4, P3
	MOV	P1, #0004
	LCALL	RET
	MOV	R5, P3
	LCALL	RET
	ADD	A, R3
	ADD	A, R4
	ADD	A, R5
	MOV	P1, A
	LCALL	RET
	LJMP	FIN
RET:	MOV	R2,#28H
PACA: 	MOV	R1, #0FAH
ACA:	MOV	R0, #0FAH
AQUI:	DJNZ 	R0,AQUI
	DJNZ	R1, ACA
	DJNZ	R2,PACA
	RET
FIN:	END